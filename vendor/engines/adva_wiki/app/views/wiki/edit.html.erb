<div class="entry">
  <h2>Edit: <%= @wikipage.title %></h2>
  <% form_for :wikipage, @wikipage, :url => wikipage_path_with_home(@section, @wikipage.permalink), :html => {:method => :put} do |f| %>
    <%= f.hidden_field :updated_at, :value => @wikipage.updated_at %>
  	<%= render :partial => "form", :locals => {:f => f} %>
  <% end %>
</div>
